<Window x:Class="LABA1_FLIGHT.View.AddSC"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LABA1_FLIGHT.View"
        mc:Ignorable="d"
        Title="AddSC" Height="450" Width="800">
    <Grid>
        <Label Content="ДОБАВЛЕНИЕ КАТЕГОРИЙ МЕСТ" HorizontalAlignment="Left" Margin="115,10,0,0" VerticalAlignment="Top" Width="553" Height="81" FontSize="36"/>
        <TextBox x:Name="tbSC" HorizontalAlignment="Left" Height="121" Margin="115,96,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="553" FontSize="36"/>
        <Button x:Name="btSave" Content="Save" HorizontalAlignment="Left" Margin="115,259,0,0" VerticalAlignment="Top" Width="553" Height="57" FontSize="36" Click="btSave_Click"/>
        <Button x:Name="btCancel" Content="Cancel" HorizontalAlignment="Left" Margin="115,335,0,0" VerticalAlignment="Top" Width="553" Height="57" FontSize="36" Click="btCancel_Click"/>

    </Grid>
</Window>
